{
  "name": "cloudflare-worker-labs-service",
  "version": "1.0.0",
  "description": "Cloudflare Worker service to manage cloud lab state in D1 Database",
  "main": "src/index.ts",
  "scripts": {
    "dev": "wrangler dev --remote",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create labs-database",
    "db:init": "wrangler d1 execute labs-database --local --file=./schema.sql",
    "db:init:remote": "wrangler d1 execute labs-database --remote --file=./schema.sql",
    "secret:set:url": "wrangler secret put BACKEND_API_URL",
    "secret:set:token": "wrangler secret put BACKEND_API_TOKEN",
    "secret:list": "wrangler secret list",
    "tail": "wrangler tail",
    "deployments": "wrangler deployments list"
  },
  "keywords": [
    "cloudflare",
    "worker",
    "d1",
    "labs",
    "api"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20241127.0",
    "typescript": "^5.7.2",
    "wrangler": "^4.45.4"
  }
}
